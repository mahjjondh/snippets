import random
import pprint
lista_de_cores = []
for i in range(0,71,1):
    random_number = random. randint(0,16777215)
    hex_number = str(hex(random_number))
    hex_number ='#'+ hex_number[2:]
    lista_de_cores.append(str(hex_number))
pprint.pprint('A Random Hex Color Code is :')
for cor in lista_de_cores:
    print("'"+str(cor)+"'")
